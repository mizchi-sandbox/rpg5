Controller = Wdr.Controllers.Base.Controller
{{#camelize}}{{name}}{{/camelize}} = Wdr.UI.Components.{{#camelize}}{{name}}{{/camelize}}

module Wdr.Controllers
  class @{{#camelize}}{{name}}{{/camelize}}Controller extends Controller
    index: ->
      {{name}} = @reuse {{#camelize}}{{name}}{{/camelize}}
      {{name}}.$appendTo '#scene-root'
